<?xml version="1.0" ?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <argument name="context" xsi:type="configurableObject">
        <argument name="class" xsi:type="string">Magento\Framework\View\Element\UiComponent\Context</argument>
        <argument name="namespace" xsi:type="string">angel_raffleclient_fifty_ticket_index</argument>
    </argument>
    <argument name="data" xsi:type="array">
        <item name="js_config" xsi:type="array">
            <item name="provider" xsi:type="string">angel_raffleclient_fifty_ticket_index.angel_raffleclient_ticket_grid_data_source</item>
            <item name="deps" xsi:type="string">angel_raffleclient_fifty_ticket_index.angel_raffleclient_ticket_grid_data_source</item>
        </item>
        <item name="spinner" xsi:type="string">angel_raffleclient_ticket_columns</item>
    </argument>
    <dataSource name="angel_raffleclient_ticket_grid_data_source">
        <argument name="dataProvider" xsi:type="configurableObject">
            <argument name="class" xsi:type="string">Angel\RaffleClient\Ui\DataProvider\Fifty\Form\TicketsDataProvider</argument>
            <argument name="name" xsi:type="string">angel_raffleclient_ticket_grid_data_source</argument>
            <argument name="primaryFieldName" xsi:type="string">ticket_id</argument>
            <argument name="requestFieldName" xsi:type="string">id</argument>
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="component" xsi:type="string">Magento_Ui/js/grid/provider</item>
                    <item name="update_url" path="mui/index/render" xsi:type="url"/>
                    <item name="storageConfig" xsi:type="array">
                        <item name="indexField" xsi:type="string">ticket_id</item>
                    </item>
                </item>
            </argument>
        </argument>
    </dataSource>
    <listingToolbar name="listing_top">
        <argument name="data" xsi:type="array">
            <item name="config" xsi:type="array">
                <item name="sticky" xsi:type="boolean">true</item>
            </item>
        </argument>
        <exportButton class="Magento\Ui\Component\ExportButton">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="displayArea" xsi:type="string">dataGridActions</item>
                    <item name="options" xsi:type="array">
                        <item name="cvs" xsi:type="array">
                            <item name="value" xsi:type="string">csv</item>
                            <item name="label" xsi:type="string" translate="true">CSV</item>
                            <item name="url" xsi:type="string">angel_raffleclient/ticket/exportCSV/type/fifty</item>
                        </item>
                    </item>
                </item>
            </argument>
        </exportButton>
        <bookmark name="bookmarks"/>
        <columnsControls name="columns_controls"/>
        <filters name="listing_filters"/>
        <paging name="listing_paging"/>
    </listingToolbar>
    <columns name="angel_raffleclient_ticket_columns">
        <argument name="data" xsi:type="array"/>
        <selectionsColumn name="ids">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="indexField" xsi:type="string">ticket_id</item>
                </item>
            </argument>
        </selectionsColumn>
        <column name="ticket_id">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">text</item>
                    <item name="sorting" xsi:type="string">asc</item>
                    <item name="label" translate="true" xsi:type="string">ID</item>
                </item>
            </argument>
        </column>
        <actionsColumn name="product_actions" class="Angel\RaffleClient\Ui\Component\Listing\Tickets\Columns\ProductActions" sortOrder="1">
            <settings>
                <indexField>product_id</indexField>
                <label translate="true">Raffle Name</label>
            </settings>
        </actionsColumn>
        <actionsColumn name="order_actions" class="Angel\RaffleClient\Ui\Component\Listing\Tickets\Columns\OrderActions" sortOrder="10">
            <settings>
                <indexField>order_id</indexField>
                <label translate="true">Order</label>
            </settings>
        </actionsColumn>
        <actionsColumn name="customer_actions" class="Angel\RaffleClient\Ui\Component\Listing\Tickets\Columns\CustomerActions" sortOrder="20">
            <settings>
                <indexField>customer_id</indexField>
                <label translate="true">Customers</label>
            </settings>
        </actionsColumn>
        <column name="start" sortOrder="30">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">text</item>
                    <item name="label" translate="true" xsi:type="string">start</item>
                </item>
            </argument>
        </column>
        <column name="end" sortOrder="40">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">text</item>
                    <item name="label" translate="true" xsi:type="string">end</item>
                </item>
            </argument>
        </column>
        <column name="status" sortOrder="50">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">text</item>
                    <item name="label" translate="true" xsi:type="string">status</item>
                </item>
            </argument>
        </column>
        <column name="serial" sortOrder="50">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">text</item>
                    <item name="label" translate="true" xsi:type="string">serial</item>
                </item>
            </argument>
        </column>
        <column name="winning_price" class="Magento\Catalog\Ui\Component\Listing\Columns\Price" sortOrder="60">
            <settings>
                <addField>false</addField>
                <filter>textRange</filter>
                <label translate="true">Winning Prizes</label>
            </settings>
        </column>
        <column name="winning_numbers" sortOrder="70">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="truncate" xsi:type="number">50</item>
                    <item name="nl2br" xsi:type="boolean">true</item>
                    <item name="escape" xsi:type="boolean">true</item>
                </item>
            </argument>
            <settings>
                <filter>text</filter>
                <label translate="true">Winning Numbers</label>
            </settings>
        </column>
        <column name="created_at" class="Magento\Ui\Component\Listing\Columns\Date" component="Magento_Ui/js/grid/columns/date" sortOrder="50">
            <settings>
                <filter>dateRange</filter>
                <dataType>date</dataType>
                <label translate="true">Purchased At</label>
            </settings>
        </column>
        <column name="reveal_at" class="Magento\Ui\Component\Listing\Columns\Date" component="Magento_Ui/js/grid/columns/date" sortOrder="50">
            <settings>
                <filter>dateRange</filter>
                <dataType>date</dataType>
                <label translate="true">Reveal At</label>
            </settings>
        </column>
    </columns>
</listing>